  "main": "server.js",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
